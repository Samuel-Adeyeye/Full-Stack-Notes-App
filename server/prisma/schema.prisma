generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // url= env("postgresql://postgres:silentwords@localhost:5500/notes_app")
}

model Note {
  id      Int      @id @default(autoincrement())
  title   String
  content String
  createdAt DateTime @default(now())
}